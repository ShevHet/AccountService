{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AccountService": {
    "Pagination": {
      "DefaultPage": 1,
      "MinPageSize": 1,
      "MaxPageSize": 100
    },
    "Commission": {
      "Rate": 0.005,
      "Min": 10,
      "Max": 1000,
      "MinDepositRate": 0.01,
      "MaxCreditRate": -0.01
    },
    "Validation": {
      "MaxDescriptionLength": 500
    },
    "Memory": {
      "MaxInMemoryAccounts": 1000
    }
  },
  "HealthChecks": {
    "Memory": {
      "ThresholdBytes": 104857600
    }
  },
  "Jwt": {
    "Authority": "http://keycloak:8080/realms/master",
    "Audience": "account-service"
  }
}